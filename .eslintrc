{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "extends": [],
  "plugins": [
    "eslint:recommended",
    "react/recommended",
    "prettier/react",
    "typescript-eslint/recommended",
    "eslint-plugin-simple-sort",
    "prettier/recommended",
    "prettier/@typescript-eslint"

  ],
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "error",
    "react/props-types": "off"
  }
}